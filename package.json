    "oicq": "^2.3.1",
    "picocolors": "^1.0.0",
    "pm2": "^5.2.0",
    "puppeteer": "^15.5.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
