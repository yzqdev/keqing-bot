    "got": "^14.2.1",
    "icqq": "^0.6.6",
    "lowdb": "^7.0.1",
    "picocolors": "^1.0.0",
    "pm2": "^5.3.1",
    "puppeteer": "^22.8.0",
    "timeago.js": "^4.0.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^5.0.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.12.11",
    "@types/node-cron": "^3.0.11",
    "esno": "^4.7.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  }
