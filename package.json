{
  "name": "keqing-bot",
  "version": "0.1.2",
  "description": "A simple qqbot",
  "repository": {
    "url": "https://github.com/yzqdev/keqing-bot.git"
  },
  "license": "GPL",
  "type": "module",
  "author": "yzqdev",
  "main": "bin/cli.mjs",
  "files": [
    "dist",
    "files"
  ],
  "scripts": {
    "back": "tsup&&pm2 restart bot.json",
    "build": "tsup",
    "unbuild": "unbuild",
    "dev": "esno src/index.ts",
